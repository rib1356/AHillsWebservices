
@{
    ViewBag.Title = " Batch Step UpLoad";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Read File onto Server</h2>

<hr />
@using (Html.BeginForm("GetFile", "StepUpload", FormMethod.Post, new { enctype = "multipart/form-data" }))
{

    <div class="form-group">
        <input type="file" id="file" name="file" />
    </div>

    <div class="form-group">
        <input type="submit" name="Read File" value="Read File" />
    </div>
}

